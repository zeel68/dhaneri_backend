{
    "version": 2,
    "buildCommand": "npm install --legacy-peer-deps && vercel build",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}